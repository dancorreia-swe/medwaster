{
  "expo": {
    "name": "Medwaster",
    "slug": "medwaster",
    "scheme": "medwaster",
    "version": "1.0.0",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "resizeMode": "contain",
          "imageWidth": 160
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Precisamos do microfone para você enviar perguntas por áudio ao tutor."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/mascot-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.medwaster.app",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Precisamos do microfone para você enviar perguntas por áudio ao tutor.",
        "NSPhotoLibraryUsageDescription": "Precisamos acessar sua galeria para você escolher ou alterar sua foto de perfil.",
        "NSPhotoLibraryAddUsageDescription": "Precisamos salvar a foto escolhida no seu dispositivo ao atualizar seu perfil.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.medwaster.app",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "RECORD_AUDIO"
      ]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b833ff3c-d533-424d-af97-88adfba41d00"
      }
    },
    "owner": "danielcorreia-swe"
  }
}
