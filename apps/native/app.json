{
  "expo": {
    "name": "Medwaster",
    "slug": "medwaster",
    "scheme": "medwaster",
    "version": "1.0.0",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      "expo-font",
      [
        "expo-asset",
        {
          "assets": "./assets/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true,
      "autolinkingModuleResolution": true
    },
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/mascot-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.medwaster.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.medwaster.app",
      "edgeToEdgeEnabled": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "10a3bf06-57dc-4ef1-bcbd-817970c07582"
      }
    }
  }
}
