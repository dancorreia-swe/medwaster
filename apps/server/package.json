{
  "name": "server",
  "main": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot src/index.ts",
    "dev:worker": "bun run --hot src/workers/index.ts",
    "start": "bun run dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun run src/db/seeds/index.ts",
    "db:reset": "bun run src/db/reset.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:start": "docker compose up -d",
    "db:watch": "docker compose up",
    "db:stop": "docker compose stop",
    "db:down": "docker compose down",
    "gamification:assign-missions": "bun run src/scripts/assign-missions.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.52",
    "@ai-sdk/openai-compatible": "^1.0.27",
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/provider-utils": "^3.0.17",
    "@aws-sdk/client-s3": "^3.927.0",
    "@aws-sdk/s3-request-presigner": "^3.927.0",
    "@better-auth/expo": "^1.3.4",
    "@blocknote/server-util": "^0.41.1",
    "@blocknote/xl-pdf-exporter": "^0.40.0",
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/eden": "^1.4.4",
    "@elysiajs/openapi": "^1.4.11",
    "@langchain/community": "^1.0.4",
    "@langchain/core": "^0.3.78",
    "@langchain/textsplitters": "^0.1.0",
    "@mozilla/readability": "^0.6.0",
    "@react-email/components": "0.5.6",
    "@react-email/render": "^1.3.1",
    "@react-pdf/renderer": "^4.3.1",
    "@sinclair/typebox": "^0.34.41",
    "@types/nodemailer": "^7.0.1",
    "@types/puppeteer": "^7.0.4",
    "ai": "^5.0.72",
    "better-auth": "^1.3.4",
    "bullmq": "^5.61.0",
    "color": "^5.0.2",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.2",
    "drizzle-seed": "^0.3.1",
    "drizzle-typebox": "^0.3.3",
    "elysia": "^1.2.25",
    "file-type": "^21.0.0",
    "jsdom": "^27.2.0",
    "jspdf": "^3.0.3",
    "logixlysia": "^5.3.0",
    "nodemailer": "^7.0.6",
    "openai": "^6.4.0",
    "pdf-parse": "^1",
    "pdfjs-dist": "^5.4.394",
    "pg": "^8.14.1",
    "puppeteer": "^24.30.0",
    "qrcode": "^1.5.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "ulid": "^3.0.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bun": "^1.2.23",
    "@types/pg": "^8.11.11",
    "@vitest/ui": "^3.2.4",
    "drizzle-kit": "^0.31.2",
    "react-email": "4.3.0",
    "tsdown": "^0.12.9",
    "typescript": "^5.8.2",
    "vitest": "^3.2.4"
  }
}
