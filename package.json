{
  "name": "medwaster",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "dev:all": "turbo run dev dev:worker --filter='./apps/*' --concurrency=10",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "test": "turbo test",
    "test:watch": "turbo test:watch",
    "test:ui": "turbo test:ui",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F server dev",
    "dev:worker": "turbo -F server dev:worker",
    "db:push": "turbo -F server db:push",
    "db:studio": "turbo -F server db:studio",
    "db:seed": "turbo -F server db:seed",
    "db:reset": "turbo -F server db:reset",
    "db:generate": "turbo -F server db:generate",
    "db:migrate": "turbo -F server db:migrate",
    "db:start": "turbo -F server db:start",
    "db:watch": "turbo -F server db:watch",
    "db:stop": "turbo -F server db:stop",
    "db:down": "turbo -F server db:down"
  },
  "devDependencies": {
    "turbo": "^2.5.4"
  },
  "overrides": {
    "@sinclair/typebox": "^0.34.41",
    "@react-native/assets-registry": "0.76.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "packageManager": "bun@1.3.0",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1"
  }
}
